<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
				
				
	
				
   	
   				
		<bean id="book1" class="com.spring.collectionMap.Books" >
   			
   			<property name="id" value="100" />
   			
   			<property name="bookName" value="Demo Book 1" />
   			
   			
   					<property name="chapters">
	   						<map>
	   								<entry key="1" value="chapter Demo 1" ></entry>
	   								<entry key="2" value="chapter Demo 2" ></entry>
	   								<entry key="3" value="chapter Demo 3" ></entry>
	   								<entry key="4" value="chapter Demo 4" ></entry>
	   								<entry key="5" value="chapter Demo 5" ></entry>
	   						</map>
					</property>
   				
   		</bean>
	
 
    
</beans>
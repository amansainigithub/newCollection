<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
				
				
	
				
   	
   				
		<bean id="book1" class="com.spring.depcollections.Book" >
   			
   			<property name="id" value="100" />
   			
   			<property name="name" value="Demo Book 1" />
   			
   			
   					<property name="chapters">
   						<list>
	   						<value>chapter 1</value>
	   						<value>chapter 2</value>
	   						<value>chapter 3</value>
	   						<value>chapter 4</value>
	   						<value>chapter 5</value>
   						</list>
					</property>
   				
   		</bean>
	
 
    
</beans>